<body>
    <!-- HEADER -->
    <header id="header">
        <div class="logo">
            <img src="~/images/Cricket League.png" alt="" />
            <span>Place for Cricket Patriots</span>
        </div>
    </header>
    <div class="clear"></div>
    <section id="submenu-container">
        <a id="mobile-menu" href="#">Sub Menu</a>
        <nav id="submenu">
            <ul>
                <li>
                    @Html.ActionLink("Home","Home")
                </li>
                <li>
                    @Html.ActionLink("Fantacy","SignIn")
                </li>
                <li>
                    @Html.ActionLink("Upcoming Matches","UpcomingMatches")
                </li>
                <li>
                    @Html.ActionLink("About Us","AboutUs")
                </li>
                <li>
                    @Html.ActionLink("Contact Us","ContactUs")
                </li>
            </ul>
        </nav>
        <!-- LOGIN MENU -->
        <nav id="submenu-login">
            <ul>
                <li>
                    @Html.ActionLink("Register","Registration")
                </li>
                <li>
                    @Html.ActionLink("Login","SignIn")
                </li>
            </ul>
        </nav>
    </section>
    <div class="clear"></div>
    <!-- MAIN MENU -->
  
    <div>
        @RenderBody()
    </div>
    <section>
        <footer id="footer">
            <div id="footer-widgets">
                <!-- WIDGET 1 -->
                <div class="footer-widget first-clmn">
                    <h5>Recent Posts</h5>
