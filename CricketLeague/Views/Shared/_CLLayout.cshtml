                    <a href="#">
                        <img class="social-icon" src="~/images/social-icons/youtube.png" alt="YouTube" />
                    </a>
                </li>
            </ul>
        </footer>
        <!-- BACK TO TOP BUTTON -->
        <a href="#" class="back-to-top"></a>
    </section>
    <!-- JS FILES -->
    <script type="text/javascript" src="~/Scripts/js/jquery.js"></script>
    <script type="text/javascript" src="~/Scripts/js/backstretch.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery.flexslider.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jflickrfeed.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery.bxslider.min.js"></script>
    <script type="text/javascript" src="~/Scripts/js/jquery.colorbox-min.js"></script>
    <!-- BG IMAGE -->
    <script type="text/javascript">
        jQuery(window).load(function () {
            "use strict";
            jQuery('body').backstretch("~/images/photos/bg-blurred.jpg");
        });
    </script>
    <!-- SPONSORS SLIDER -->
    <script type="text/javascript">
        jQuery(document).ready(function () {
            jQuery('#logos').bxSlider({
                slideWidth: 235,
                pager: false,
                minSlides: 2,
                maxSlides: 5,
                infiniteLoop: false,
                hideControlOnEnd: true,
                slideMargin: 10
            });
        });
    </script>
    <!-- FIXTURE SLIDER -->
    <script type="text/javascript">
        jQuery(document).ready(function () {
            "use strict";
            jQuery('#fixture').bxSlider({
                pager: false,
                infiniteLoop: false,
                hideControlOnEnd: true
            });
        });
    </script>
    <script src="~/Scripts/js/custom.js" type="text/javascript"></script>
